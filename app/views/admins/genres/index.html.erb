<%# ジャンル一覧＆追加画面%>
<div class="container">
    <P>ジャンル一覧・追加</P>
</div>
<div class="container">
    <div class="row">
        <%= form_with model: @admin,method: :post do |f| %>
            <div class="form-group">
              <a>ジャンル名</a>
              <%= f.text_field :title %>
              <%= f.submit '新規登録', class:"btn btn-success" %>
            </div>
            <div class="form_group">

            </div>
        <% end %>
    </div>
</div>